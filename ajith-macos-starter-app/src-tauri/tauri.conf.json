{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "ajith-macos-starter",
  "version": "0.0.0",
  "identifier": "com.ajith-macos-starter.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "ajith-macos-starter",
        "width": 480,
        "height": 520,
        "minWidth": 480,
        "minHeight": 520,
        "resizable": true,
        "fullscreen": false,
        "visible": false,
        "decorations": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "label": "overlay",
        "transparent": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.png"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "sql": {
      "preload": ["sqlite:ajith-macos-starter.db"]
    }
  }
}